<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" styleClass="anchor" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uet.hal.LoadingMenu">
  <padding>
    <Insets bottom="10" left="25" right="25" top="25" />
  </padding>
  <stylesheets>
    <URL value="@loadingMenu.css" />
  </stylesheets>
   <children>
      <Button id="goButton" fx:id="goButton" layoutX="482.0" layoutY="475.0" mnemonicParsing="false" onAction="#switchToMainMenu" style="-fx-background-image: url(&quot;@Pictures/goButtonNormal.png&quot;);" text="GO">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </padding>
      </Button>
      <TextField id="tipTextField" fx:id="tipTextField" alignment="CENTER" editable="false" layoutX="204.0" layoutY="310.0" onContextMenuRequested="#changeTip" onMouseClicked="#changeTip" prefHeight="70.0" prefWidth="615.0" style="-fx-background-color: none;" stylesheets="@loadingMenu.css" text="fsdfsdgs">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </TextField>
      <TextField id="guideTip" alignment="CENTER" layoutX="300.0" layoutY="380.0" prefHeight="26.0" prefWidth="422.0" style="-fx-background-color: none;-fx-text-inner-color: white;" text="(click to change tip)" />
   </children>

</AnchorPane>
