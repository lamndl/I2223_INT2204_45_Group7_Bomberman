<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" styleClass="background" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scenes.CampaignScene">
  <stylesheets>
    <URL value="@campaignScene.css" />
  </stylesheets>
   <children>
      <VBox fx:id="vb" layoutX="119.0" layoutY="134.0" maxHeight="500.0" prefHeight="500.0" prefWidth="225.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </VBox>
      <ImageView fx:id="mapView" fitHeight="330.0" fitWidth="500.0" layoutX="400.0" layoutY="134.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="goButton" alignment="CENTER" layoutX="622.0" layoutY="547.0" mnemonicParsing="false" onAction="#goChoosingCharacterScene" text="GO">
         <font>
            <Font size="20.0" />
         </font>
         <padding>
            <Insets left="15.0" right="15.0" />
         </padding></Button>
      <ScrollBar fx:id="sc" disable="true" layoutX="347.0" layoutY="132.0" opacity="0.0" orientation="VERTICAL" prefHeight="500.0" prefWidth="14.0" />
      <Text fx:id="mapChoosingNotification" fill="WHITE" layoutX="400.0" layoutY="486.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You are choosing: Unknown map">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Button fx:id="backButton" layoutX="935.0" layoutY="35.0" mnemonicParsing="false" onAction="#goMainMenu" text="Back">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Button>
   </children>
</AnchorPane>
